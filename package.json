{
  "name": "nks-bob-frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev:frontend": "MILJO=local vite",
    "dev:server": "pnpm run --filter server dev",
    "dev": "pnpm run \"/^dev:(frontend|server)$/\"",
    "localnais:frontend": "MILJO=localnais vite --host",
    "localnais:server": "MILJO=localnais pnpm run --filter server dev",
    "localnais": "pnpm run \"/^localnais:(frontend|server)$/\"",
    "build:frontend": "tsc -b && vite build",
    "build:server": "pnpm run --filter server build",
    "build": "pnpm run \"/^build:(frontend|server)$/\"",
    "lint": "eslint .",
    "preview": "vite preview",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
    "preinstall": "npx only-allow pnpm"
  },
  "dependencies": {
    "@amplitude/analytics-browser": "^2.17.8",
    "@navikt/aksel-icons": "^8.1.0",
    "@navikt/ds-css": "^8.1.0",
    "@navikt/ds-react": "^8.1.0",
    "@navikt/ds-tailwind": "^8.1.0",
    "@unhead/dom": "^2.0.10",
    "@unhead/react": "^2.0.10",
    "date-fns": "^4.1.0",
    "dotenv": "^16.5.0",
    "mdast-util-find-and-replace": "^3.0.2",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-error-boundary": "^6.0.0",
    "react-hotkeys-hook": "^5.1.0",
    "react-markdown": "^9.1.0",
    "react-router": "^7.6.2",
    "rehype-raw": "^7.0.0",
    "rehype-stringify": "^10.0.1",
    "remark": "^15.0.1",
    "remark-breaks": "^4.0.0",
    "remark-gfm": "^4.0.1",
    "remark-rehype": "^11.1.2",
    "remark-stringify": "^11.0.0",
    "server": "workspace:*",
    "strip-markdown": "^6.0.0",
    "swr": "^2.3.3",
    "ts-debounce": "^4.0.0",
    "unist-util-visit": "^5.0.0",
    "zustand": "^5.0.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.0",
    "@tailwindcss/postcss": "^4.1.18",
    "@types/mdast": "^4.0.4",
    "@types/node": "^24.10.1",
    "@types/react": "^19.1.6",
    "@types/react-dom": "^19.1.6",
    "@types/unist": "^3.0.3",
    "@vitejs/plugin-react": "^4.5.1",
    "eslint": "^9.28.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.2.0",
    "postcss": "^8.5.4",
    "prettier": "^3.5.3",
    "prettier-plugin-organize-imports": "^4.1.0",
    "prettier-plugin-tailwindcss": "^0.7.2",
    "tailwindcss": "^4.1.18",
    "terser": "^5.41.0",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.33.1",
    "vite": "^6.3.5"
  }
}